{
    "env": {
        "browser": true,
        "es2021": true,
        "cypress/globals": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-essential",
        "plugin:cypress/recommended",
        "plugin:prettier/recommended",
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "cypress",
        "prettier"
    ],
    "rules": {
        "vue/no-reserved-component-names": "off",
        "vue/multi-word-component-names": "off"
    }
}